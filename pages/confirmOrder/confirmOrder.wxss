.con {
  background: #fff;
  padding-bottom: 40rpx;
}

.con_le {
  float: left;
}

.con_ri {
  width: calc(100% - 190rpx);
  float: left;
  margin-left: 30rpx;
  margin-top: -16rpx;
}

.left {
  margin-right: 0px;
  margin-left: 10rpx;
}

.con_le > image {
  width: 160rpx;
  height: 160rpx;
  background-color: #f9de4a;
  border-radius: 6rpx;
}

.ri_le {
  float: left;
  width: 329rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.ri_ri {
  float: right;
  margin-left: 40rpx;
  margin-top: 10rpx;
}

.ri_le > text {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #1e1e1e;
}

.ri_sell {
  font-family: DINNextLTPro-Medium;
  font-size: 32rpx;
  line-height: 44rpx;
  color: #1e1e1e;
}

.ri_orgin {
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  line-height: 44rpx;
  color: #969696;
  text-align: right;
  text-decoration: line-through;
}

.con_select > image {
  width: 12rpx;
  height: 22rpx;
  margin-right: 20rpx;
}

.menu {
  display: inline-block;
  float: left;
  font-size: 24rpx;
  color: #969696;
}

.menu_num {
  display: inline-block;
  float: right;
  font-size: 24rpx;
  color: #969696;
  margin-right: 6rpx;
  margin-top: 40rpx;
}

.con_attention {
  font-family: PingFangSC-Regular;
  font-size: 22rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: left;
  color: #969696;
  background: #f8f8f8;
  padding: 0 40rpx;
  box-sizing: border-box;
  width: 100%;
}

.con_row {
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  padding: 0 40rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #f5f5f5;
}

.con_row1 {
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  padding: 0 40rpx;
  box-sizing: border-box;
}

.row_text {
  float: left;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #333;
}

.row_show {
  float: right;
}

.row_symbol {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  line-height: 38rpx;
  text-align: center;
  border: solid 1px #dbdbdb;
  border-radius: 50%;
}

.row_sum {
  display: inline-block;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #000;
  height: 40rpx;
  width: 72rpx;
  line-height: 40rpx;
  text-align: center;
}

.con_select {
  margin-right: -14rpx;
}

.con_select > text {
  color: #969696;
  font-size: 28rpx;
  margin-right: 20rpx;
}

.beizhu {
  color: #333;
  font-size: 28rpx;
}

.tex > textarea {
  width: 95% !important;
  color: #969696;
  font-size: 28rpx;
  box-sizing: border-box;
  padding: 10rpx;
  line-height: 50rpx;
  border: 1rpx solid #ddd;
}

.con_ft {
  position: absolute;
  bottom: 0rpx;
  left: 0rpx;
  display: flex;
  box-sizing: border-box;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
}

.con_top {
  padding: 40rpx;
  box-sizing: border-box;
  border-top: 2rpx solid #fafafa;
}

.con_total {
  flex: 1;
  color: #323232;
  padding-left: 40rpx;
  font-size: 28rpx;
  border: 1rpx solid #e5e5e4;
}

.con_total > text {
  color: #eb4c45;
  font-size: 36rpx;
  margin-left: 19rpx;
}

.con_pay {
  flex: 1;
  text-align: center;
  background-color: #f9de4a;
  color: #2d2d2d;
  font-size: 32rpx;
}

.zhe {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  width: 100%;
  height: 100%;
  /* background-color: #313131; */
  z-index: 100;
  opacity: 0.5;
}

.mask {
  width: 100%;
  background: #fff;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 999;
}

.mask ul {
  display: block;
  width: 100%;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
  list-style: none;
}

.mask ul li {
  display: block;
  font-size: 26rpx;
  padding: 30rpx;
  color: #333;
  text-align: left;
  border-bottom: 1px solid #eee;
}

.close {
  width: 90%;
  font-size: 30rpx;
  padding: 10rpx 0;
  color: #fff;
  text-align: center;
  background: #f9de4a;
  border-radius: 0;
  border: 0;
  margin-top: 50rpx;
  border-radius:10rpx;
  position: fixed;
  bottom: 60rpx;
  right: 40rpx;
}

/*优惠卷  */

page {
  background: #f8f8f8;
}

.cou_list {
  background: #fff;
  box-sizing: border-box;
  border-radius: 5px;
  width: 100%;
  margin-top: 20rpx;
}

.list_le {
  float: left;
  width: 66%;
  padding: 30rpx;
  padding-right: 0;
}

.container1 {
  height: 100%;
  padding: 40rpx;
  background: #f8f8f8;
  box-sizing: border-box;
}

.after_ {
  float: left;
  width: 0px;
  height: 220rpx;
  position: relative;
  border-left: 1px dashed #e0e0e0;
}

.after_::after {
  display: block;
  content: "";
  position: absolute;
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background: #f8f8f8;
  top: -10rpx;
  left: -10rpx;
}

.after_::before {
  display: block;
  content: "";
  position: absolute;
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background: #f8f8f8;
  bottom: -10rpx;
  left: -10rpx;
}

.list_ri {
  float: right;
  text-align: center;
