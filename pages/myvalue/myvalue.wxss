page{
    background-color: #f8f8f8;
}
.head{
    background-color: white;
    width: 80%;
    margin: 60rpx auto;
    padding: 20rpx 30rpx;
    border-radius: 10rpx;
}
.head>text{
    font-size: 18rpx;
    color: rgb(114, 114, 114);
}
.head>view>text{
    font-size: 60rpx;
    color: #e9423b;
}
.head>view>text:nth-of-type(1){
    font-size: 40rpx;
    color: #e9423b;
    font-weight: bolder;
}
.head>view{
    position: relative;
}
.carsh{
    position: absolute;
    right: 30rpx;
    top: 10rpx;
    font-size: 26rpx !important;
    color: #e9423b;
    font-weight: bolder;
    padding: 10rpx 20rpx;
    border: 1rpx solid #f9dd4b;
    border-radius: 10rpx;
}
.view_set {
    margin-top: 100rpx;
  }
  
  .set_li {
    box-sizing: border-box;
    width: 90%;
    padding: 0 40rpx;
    margin-left: 5%;
    height: 100rpx;
    line-height: 100rpx;
    background: #fff;
    border-radius: 5px;
    margin-bottom: 20rpx;
  }
  
  .li_name {
    float: left;
    width: 80%;
    color: #1e1e1e;
    font-size: 28rpx;
  }
  
  .set_li > image {
    float: right;
    width: 11rpx;
    height: 20rpx;
    margin-top: 42rpx;
  }
  